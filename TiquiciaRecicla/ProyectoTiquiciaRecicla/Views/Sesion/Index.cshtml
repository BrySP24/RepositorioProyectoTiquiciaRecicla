@model IEnumerable<ProyectoTiquiciaRecicla.Models.TBL_Usuario>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CH_Nombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CH_Apellido_1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CH_apellido_2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CH_Correo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CH_Clave)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CH_Telefono)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CH_Direccion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CAT_Provincias)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CH_Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CH_Apellido_1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CH_apellido_2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CH_Correo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CH_Clave)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CH_Telefono)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CH_Direccion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CAT_Provincias.CH_Nombre)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
